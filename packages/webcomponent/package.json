{
  "name": "autoimg-webcomponent",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./define": {
      "types": "./dist/auto-img-element.define.d.ts",
      "import": "./dist/auto-img-element.define.js",
      "require": "./dist/auto-img-element.define.cjs"
    }
  },
  "customElements": "custom-elements.json",
  "files": [
    "dist",
    "custom-elements.json"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/auto-img-element.define.ts --format esm,cjs,iife --dts --global-name AutoImg --minify",
    "dev": "tsup src/index.ts src/auto-img-element.define.ts --format esm,cjs,iife --dts --global-name AutoImg --watch",
    "demo": "npm run build && npx serve ./demo",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "@types/react": "*"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "dependencies": {
    "autoimg-core": "*"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^2.0.0",
    "jsdom": "^25.0.0",
    "vitest": "^2.0.0"
  }
}
