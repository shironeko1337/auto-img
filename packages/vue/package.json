{
  "name": "autoimg-vue",
  "version": "0.1.0",
  "description": "Vue component wrapper for AutoImg - content-aware image resizing",
  "author": "Liyi Zhang",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:shironeko1337/auto-img.git",
    "directory": "packages/vue"
  },
  "homepage": "https://github.com/shironeko1337/auto-img",
  "bugs": {
    "url": "https://github.com/shironeko1337/auto-img/issues"
  },
  "keywords": [
    "vue",
    "component",
    "image",
    "responsive",
    "focus",
    "crop",
    "auto-img",
    "content-aware",
    "vue-component"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "vue": "^3.5.13",
    "autoimg": "*"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/ui": "^2.0.0",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^15.11.7",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vitest": "^2.0.0",
    "vue-tsc": "^2.1.10"
  }
}
